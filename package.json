{
    "name": "2_shop",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.3.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.11.41",
        "@types/react": "^18.0.14",
        "@types/react-dom": "^18.0.5",
        "@types/react-router": "^5.1.18",
        "@types/react-router-dom": "^5.3.3",
        "antd": "^4.21.3",
        "jest-fetch-mock": "^3.0.3",
        "madge": "^5.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-intl": "^6.0.5",
        "react-router-dom": "^6.3.0",
        "react-scripts": "^5.0.1",
        "typescript": "^4.7.4",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "testAll": "react-scripts test --watchAll=false",
        "eject": "react-scripts eject",
        "test:coverage": "react-scripts test --coverage --watchAll=false",
        "circular": "madge --circular src",
        "circular:img": "madge --circular --image circularDependencies.svg src"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*.d.ts"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 30,
                "functions": 30,
                "lines": 30,
                "statements": 50
            }
        }
    },
    "madge": {
        "fontSize": "10px",
        "fileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx"
        ],
        "graphVizOptions": {
            "G": {
                "rankdir": "LR"
            }
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@tailwindcss/aspect-ratio": "^0.4.0",
        "autoprefixer": "^10.4.7",
        "postcss": "^8.4.14",
        "prettier": "2.7.1",
        "sass": "^1.52.3",
        "tailwindcss": "^3.1.3"
    }
}
